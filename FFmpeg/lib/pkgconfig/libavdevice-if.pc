Libs: -L${libdir}  -lavdevice-if
Libs.private: -lpsapi -lole32 -lstrmiids -luuid -loleaut32 -lshlwapi -lgdi32 -lm -latomic -lopenal -lvfw32 -lcaca -lmingw32 -lSDL2main -lSDL2
